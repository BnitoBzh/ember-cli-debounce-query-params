{
  "name": "ember-cli-debounce-query-params",
  "description": "Ember CLI addon to update query params with a debounce",
  "url": "https://github.com/BnitoBzh/ember-cli-debounce-query-params",
  "version": "0.0.0",
  "options": {
    "enabledEnvironments": ["development", "production"],
    "paths": [
      "addon"
    ],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "nocode": false,
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": [
      "node_modules/yuidoc-ember-theme/helpers/helpers.js"
    ]
  }
}
